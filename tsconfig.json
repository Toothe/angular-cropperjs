{
    "angularCompilerOptions": {
        "enableIvy": false
    },
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "downlevelIteration": true,
        "module": "esnext",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es2015",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ],
        "paths": {
            "ngx-cropper-js": [
                "dist/ngx-cropper-js"
            ],
            "ngx-cropper-js/*": [
                "dist/ngx-cropper-js/*"
            ]
        }
    }
}
